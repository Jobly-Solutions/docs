#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/luchho/Downloads/bravilo-main 2/node_modules/.pnpm/mintlify@4.0.525_@types+node@18.15.10_@types+react@18.0.29_react-dom@18.3.1_react@18.3.1_typescript@5.0.2/node_modules/mintlify/node_modules:/Users/luchho/Downloads/bravilo-main 2/node_modules/.pnpm/mintlify@4.0.525_@types+node@18.15.10_@types+react@18.0.29_react-dom@18.3.1_react@18.3.1_typescript@5.0.2/node_modules:/Users/luchho/Downloads/bravilo-main 2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/luchho/Downloads/bravilo-main 2/node_modules/.pnpm/mintlify@4.0.525_@types+node@18.15.10_@types+react@18.0.29_react-dom@18.3.1_react@18.3.1_typescript@5.0.2/node_modules/mintlify/node_modules:/Users/luchho/Downloads/bravilo-main 2/node_modules/.pnpm/mintlify@4.0.525_@types+node@18.15.10_@types+react@18.0.29_react-dom@18.3.1_react@18.3.1_typescript@5.0.2/node_modules:/Users/luchho/Downloads/bravilo-main 2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mintlify/index.js" "$@"
else
  exec node  "$basedir/../mintlify/index.js" "$@"
fi
